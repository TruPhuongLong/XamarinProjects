<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="CustomerApp.src.Views.Components.NotificationView"
    x:Name="this"
    >
    
    <Frame x:Name="ToggledNotificationView" Padding="20" BackgroundColor="Silver" BindingContext="{Binding Source={x:Reference this}}">
        <Label Text="{Binding Message}" />
    </Frame>
    
</ContentView>
